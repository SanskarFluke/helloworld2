{
  "name": "helloworld2",
  "private": true,
  "version": "0.0.0",
  "main": "electron/main.cjs",
  "scripts": {
    "copy-preload": "cp preload.js dist/",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "cross-env NODE_ENV=development electron .",
    "electron:build": "vite build && cross-env NODE_ENV=production electron .",
    "dist": "vite build && npm run copy-preload && electron-builder",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.2.0",
    "axios": "^1.10.0",
    "better-sqlite3": "^12.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "is-online": "^11.0.0",
    "node-ssh": "^13.2.1",
    "pg": "^8.16.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "react-select-country-list": "^2.2.3",
    "sqlite3": "^5.1.7",
    "sudo-prompt": "^9.2.1",
    "usb": "^2.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.6.0",
    "cross-env": "^7.0.3",
    "electron": "^37.2.1",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.0.4"
  },
"build": {
  "appId": "com.example.helloworld2",
  "productName": "HelloWorld2",
  "files": [
    "dist/**/*",
    {
      "from": "dist/preload.js",
      "to": "preload.js"
    },
    "electron/**/*",
    "node_modules/**/*",
    "package.json"
  ],
  "mac": {
    "target": [
      {
        "target": "dmg",
        "arch": ["x64"]
      }
    ],
    "category": "public.app-category.utilities"
  },
  "dmg": {
    "format": "UDZO",
    "background": null,
    "window": {
      "width": 540,
      "height": 380
    },
    "contents": [
      {
        "x": 130,
        "y": 220
      },
      {
        "x": 410,
        "y": 220,
        "type": "link",
        "path": "/Applications"
      }
    ]
  }
}

}
